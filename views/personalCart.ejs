<%- include('partials/header') -%>
   <div>
       <% loggedInUser.personalCart.forEach(elem =>{ %>
        <p>
            <%= elem.book.slug %>
        </p>
        <h1> 
            <%= elem.book.title %>
        </h1>
        <h2> 
            <%= elem.book.author %>
        </h2>
        <h3> 
            <%= elem.book.description %>
        </h3>
        <h4> 
            <%= elem.book.thumbnailURL %>
        </h4>
        <h5> 
            <%= elem.book.price %>
        </h5>
        <h5> 
            Ordered: <%= elem.quantity %>
        </h5>
        <a href='/books/<%= elem.book.slug %>/remove'>
          Remove from cart  
        </a>
       <%}); %>
   </div>
</body>
</html>
