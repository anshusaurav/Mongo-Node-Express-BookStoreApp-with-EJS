<%- include('partials/header') -%>
   <div>
       <% loggedInUser.wishList.forEach(book =>{ %>
        <p>
            <%= book.slug %>
        </p>
        <h1> 
            <%= book.title %>
        </h1>
        <h2> 
            <%= book.author %>
        </h2>
        <h3> 
            <%= book.description %>
        </h3>
        <h4> 
            <%= book.thumbnailURL %>
        </h4>
        <h5> 
            <%= book.price %>
        </h5>
        <a href='/books/<%= book.slug %>/discard'>
            Unlike
        </a>
       <%}); %>
   </div>
</body>
</html>
